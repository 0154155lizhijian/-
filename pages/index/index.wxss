.btnGroup {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 3rem;
  background-color: #fff;
}
.buttons{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 100%;
  height: 2rem;
  background-color: #fff;
  margin-bottom: 24px;
}
.button{
  width: 30%;
  height: 2rem;
  line-height: 2rem;
  color:#d9eef7;
  background-color: #0095cd;
}
.item {
  display: flex;
  flex: 1;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  margin-top: 1px;
  margin-right: 1px;
}
.zero{
  width: 50%;
}
.orange {
  color: #fef4e9;
  background: #f78d1d;
  font-weight: bold;
}
.blue {
  color:#d9eef7;
  background-color: #0095cd;
}

table{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 100%;
  background-color: #f78d1d;
  margin-bottom: 8px;
}
th{
  border: 1px solid #b3a68c;
  height: 32px;
  line-height: 32px;
  flex: 1;
  text-align: center;
}
td{
  padding-top: 4px;
  border: 1px solid #b3a68c;
  height: 28px;
  line-height: 28px;
  flex: 1;
  text-align: center;
}
tr{
  display: flex;
  flex-direction: row;
  width: 100%;
  flex: 1;
}

.minStyle{
  background-color: red;
}

/** 修改默认的navigator点击态 **/
.navigator-hover {
    color:blue;
}
.github{
  color: green;
  font-size: 14px;
  text-align: center;
  margin-top: 5px;
}
.icon{
  vertical-align: middle;
  margin-right: 2px;
}